				var isiHtml = $('.arx-scrollbar-mask').html();
				$('body').append(isiHtml);
				$('.arx-scrollbar-content').eq(1).css({
					position: 'absolute',
					top: '10px', right: '10px',
					width: width });

			} }]);return Banner;}();


