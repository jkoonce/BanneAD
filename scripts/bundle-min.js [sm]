(function () {

	//private
	var self;var

	Banner = function () {

		function Banner() {_classCallCheck(this, Banner);
			//properties
			this.sw = 160;
			this.sh = 600;
			this.logo = $('.logo');
			this.road = $('.road');
			this.top = $('.top');
			this.bottom = $('.bottom');
